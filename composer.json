{
    "name": "marcot89/laravel-bullet",
    "description": "Bullet development for Laravel Applications",
    "keywords": [
        "laravel-bullet",
        "dynamic routes",
        "controllers",
        "crud",
        "routes",
        "actions"
    ],
    "homepage": "https://github.com/marcot89/laravel-bullet",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Marco TÃºlio",
            "email": "marcotulio.avila@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/database": "^6.0",
        "illuminate/http": "^6.0",
        "illuminate/support": "^6.0",
        "orchestra/testbench": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "MarcoT89\\Bullet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcoT89\\Bullet\\Tests\\": "tests",
            "App\\": "app"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarcoT89\\Bullet\\BulletServiceProvider"
            ],
            "aliases": {
                "Bullet": "MarcoT89\\Bullet\\Facades\\Bullet"
            }
        }
    }
}
