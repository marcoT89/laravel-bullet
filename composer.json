{
    "name": "marcot89/laravel-bullet",
    "description": "Bullet development for Laravel Applications",
    "keywords": [
        "marcot89",
        "laravel-bullet"
    ],
    "homepage": "https://github.com/marcot89/laravel-bullet",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Marco TÃºlio",
            "email": "marcotulio.avila@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "MarcoT89\\LaravelBullet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcoT89\\LaravelBullet\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarcoT89\\LaravelBullet\\LaravelBulletServiceProvider"
            ],
            "aliases": {
                "LaravelBullet": "MarcoT89\\LaravelBullet\\LaravelBulletFacade"
            }
        }
    }
}
